<%- include header %>

<div class="app-body">
  <%- include menu %>
  <main class="main">
    <!-- Breadcrumb-->
    <ol class="breadcrumb">
      <li class="breadcrumb-item">ST合约</li>
      <li class="breadcrumb-item active">获取ST的交易单位</li>
    </ol>

    <div class="container-fluid">
      <div class="animated fadeIn">
        <div class="card">
          <div class="card-header">getSTgranularity(securityToken)</div>
          <div class="card-body">
            <div class="bd-example">
              获取ST的交易最小单位
          </div>
            </div>
        </div>
        </div>
    </div>

    <%- include code %>

  </main>
</div>
<script type="text/javascript">

  function runCode() {
    getSTgranularity(config.contract.ST).then();
  }

  async function getSTgranularity(securityToken) {
    try {
      var contract = {address: securityToken, abi: abiSecurityToken};
      var st = new SecurityToken(web3, contract);
      var res  = await st.granularity();
      var result = "result:" + res + "\n";
    } catch (e) {
      var result = "error：" + e + "\n";
    }

    setResult(result);

  }

  // var Web3 = require('web3');
  $(document).ready(function() {
    var code = runCode.toString()+"\n\n";
    code += getSTgranularity.toString() + "\n\n";
    code += "runCode();\n\n";
    setCode(code);

  });

</script>
<%- include footer %>
