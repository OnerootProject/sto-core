<%- include header %>

<div class="app-body">
  <%- include menu %>
  <main class="main">
    <!-- Breadcrumb-->
    <ol class="breadcrumb">
      <li class="breadcrumb-item">STO合约</li>
      <li class="breadcrumb-item active">获取STO的配置信息</li>
    </ol>

    <div class="container-fluid">
      <div class="animated fadeIn">
        <div class="card">
          <div class="card-header">getSTODetails(sto)</div>
          <div class="card-body">
            <div class="bd-example">
              获取STO的配置信息
          </div>
            </div>
        </div>
        </div>
    </div>

    <%- include code %>

  </main>
</div>
<script type="text/javascript">

  function runCode() {
    getSTODetails(config.contract.STO).then();
  }

  async function getSTODetails(sto) {
    var res = getWeb3Account();
    var account = res.data;
    var result = "账号：" + account +"\n";
    if(res.code!=0) {
      result += res.msg+ '\n';
      setResult(result);
      return;
    }

    try {
      var contract = {address: sto, abi: abiSTO};
      var sto = new STO(web3, contract);
      res  = await sto.getSTODetails();
      result += "getSTODetails" + res + "\n";
    } catch (e) {
      result += "error：" + e + "\n";
    }

    setResult(result);

  }

  // var Web3 = require('web3');
  $(document).ready(function() {
    var code = getWeb3Account.toString()+"\n\n";
    code += getSTODetails.toString() + "\n";
    setCode(code);

  });

</script>
<%- include footer %>
